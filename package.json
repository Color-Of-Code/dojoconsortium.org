{
  "name": "dojoconsortium.org",
  "version": "1.0.0",
  "private": true,
  "description": "Recommended practices for CD",
  "repository": {
    "type": "git",
    "url": "https://github.com/devopsdojoconsortium/dojoconsortium.org.git"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "develop": "gatsby develop",
    "deploy": "node ./_deploy.js",
    "start": "npm run clean && npm run develop",
    "build": "npm run clean && cross-env INLINE_RUNTIME_CHUNK=false gatsby build --prefix-paths && mv public dist",
    "clean": "rimraf .cache public dist",
    "serve": "gatsby serve",
    "now-build": "npm run build",
    "format:fix": "prettier --write '**/*.{js,jsx,yml,json,css,md}'",
    "lint:js": "eslint --cache --ext .js,.jsx . --fix",
    "lint:fix": "concurrently 'npm run lint:js' 'npm run format:fix'",
    "lint": "eslint --cache --ext .js,.jsx .",
    "test:links": "linkinator http://localhost:8000 --silent --recurse",
    "check-links": "start-server-and-test start http://localhost:8000 test:links",
    "test:ci": "jest --watch",
    "test": "jest --coverage && echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "test:blc": "blc -r http://localhost:8000",
    "blc": "start-server-and-test start http://localhost:8000 test:blc",
    "test:unit": "npm run test",
    "test:e2e-ci": "echo 'No E2E'",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "husky install"
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "absolute",
      "options": {}
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "clsx": "^1.1.1",
    "formik": "^2.2.6",
    "gatsby": "^3.3.1",
    "gatsby-plugin-catch-links": "^3.3.0",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-image": "^1.2.0",
    "gatsby-plugin-local-search": "^2.0.0",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-material-ui": "^3.0.1",
    "gatsby-plugin-offline": "^4.3.0",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sharp": "^3.3.1",
    "gatsby-plugin-uninline-styles": "^0.2.0",
    "gatsby-remark-autolink-headers": "^3.2.0",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-image-attributes": "^0.2.8",
    "gatsby-remark-images": "^4.2.0",
    "gatsby-remark-prismjs": "^4.2.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-transformer-remark": "^4.0.0",
    "gatsby-transformer-sharp": "^3.2.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-use-flexsearch": "^0.1.1",
    "remark-image-attributes": "^0.2.1",
    "styled-components": "^5.2.3",
    "typeface-roboto": "^1.1.13",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^1.2.0",
    "broken-link-checker": "0.7.8",
    "cheerio": "^1.0.0-rc.6",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "filehound": "^1.17.4",
    "fs-extra": "^8.1.0",
    "htmlparser2": "^3.10.1",
    "husky": "^6.0.0",
    "import-sort-style-absolute": "^1.0.1",
    "jest": "^26.6.3",
    "jest-preset-gatsby": "^1.0.121",
    "linkinator": "^2.13.6",
    "lodash": "^4.17.21",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.14.0",
    "replace-in-file": "^6.2.0",
    "rimraf": "^3.0.2",
    "start-server-and-test": "1.11.0"
  }
}
